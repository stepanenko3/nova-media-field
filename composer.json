{
    "name": "stepanenko3/nova-media-field",
    "description": "A Laravel Nova field for laravel-medilibrary.",
    "keywords": [
        "laravel",
        "nova",
        "media",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/nova": "^4.22",
        "stepanenko3/nova-filemanager": "*"
    },
    "autoload": {
        "psr-4": {
            "Stepanenko3\\NovaMediaField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\NovaMediaField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://laravelsatis.com"
        },
        {
            "type": "path",
            "url": "../nova-filemanager"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "tightenco/duster": "^2.7"
    }
}
